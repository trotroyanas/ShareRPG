    "@uppy/tus": "^1.6.0",
    "@uppy/xhr-upload": "^1.6.0",
    "axios": "^0.19.2",
    "base-64": "^0.1.0",
    "buffer": "^5.6.0",
    "firebase": "^7.15.4",
    "firebase-admin": "^8.12.1",
    "jsonwebtoken": "^8.5.1",
    "npm": "^6.14.5",
    "rollup-plugin-babel": "^4.4.0",
